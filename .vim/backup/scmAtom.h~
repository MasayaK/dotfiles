#ifndef __FOOSCHEME_SCMATOM_H__
#define __FOOSCHEME_SCMATOM_H__

#include "gc_cpp.h"

#include <string>



class ScmAtom : public gc {

public:

    ScmAtom()                                              { }
    ScmAtom(ScmAtom atom)                  : Token(atom)   { }
    ScmAtom(Token token)                   : Token(token)  { }
    ScmAtom(std::string s)                 : Token(s)      { }
    ScmAtom(ScmAtomID id_)                 : Token(id_)    { }
    ScmAtom(std::string s, ScmAtomID id_)  : Token(s, id_) { }

};



#endif

#include <iostream>
#include <cstdlib>
#include <utility>


typedef std::vector<int>    VecInt;



bool
isSosuu(int n) {

    // static VecInt sosuu;
    const int END_N = n / 2;

    for (int i = 2; i < END_N; i++) {
        if (n % i == 0) {
            return false;
        }
    }

    // sosuu.push_back(n);
    return true;
}


int
main(int argc, char *argv[]) {

    const int MAX_COUNT = 10000;


    for (int i = 0; i < MAX_COUNT; i++) {
        if (isSosuu(i)) {
            std::cout << "found:" << i << std::endl;
        }
    }

    return EXIT_SUCCESS;
}



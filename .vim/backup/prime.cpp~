#include <iostream>
#include <cstdlib>
#include <utility>
#include <vector>


typedef std::vector<int>    VecInt;
const int MAX_COUNT = 10000;



bool
isPrime(int n) {

    // static VecInt prime;
    const int END_N = n / 2;

    for (int i = 2; i <= END_N; i++) {
        if (n % i == 0) {
            return false;
        }
    }

    // prime.push_back(n);
    return true;
}


int
main(int argc, char *argv[]) {

    for (int i = 2; i < MAX_COUNT; i++) {
        if (isPrime(i)) {
            std::cout << "found:" << i << std::endl;
        }
    }

    return EXIT_SUCCESS;
}

(use srfi-1)
; (use ggc.debug.trace)
(use gauche.interactive)

(define Y
  (lambda (f)
    ((lambda (proc)
       (f (lambda (arg) ((proc proc) arg))))
     (lambda (proc)
       (f (lambda (arg) ((proc proc) arg)))))))

(define fact0
  (lambda (f)
    (lambda (n)
      (if (zero? n)
        1
        (* n (f (- n 1)))))))


(define (main args)

  (if (pair? (cdr args))
    (print (cons "args:" (cdr args)) "\n"))

  (print ((fact0 f) 5))

  0)


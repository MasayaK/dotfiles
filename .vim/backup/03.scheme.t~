use strict;
use warnings;

use Test::More qw(no_plan);
use Test::Output;

use MyScheme;
my $interp = MyScheme->new();


stdout_is(
    sub { $interp->eval_from_string('(print "Hello")') },
    "Hello\n",
    'scm_func print'
);
stdout_is(
    sub { $interp->eval_from_string('(print \'a "foo" \'b "bar")') },
    writer($scheme_code),
    "afoobbar\n",
    'scm_func print'
);

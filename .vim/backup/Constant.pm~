package Text::Tokenizer::Class::Constant;

use strict;
use warnings;
use utf8;

use Carp;

use version;
our $VERSION = qv('0.0.0');


use base qw(Exporter);

our @EXPORT    = qw($BASE);
our $BASE = 'Text::Tokenizer::Class::Base';



1;
__END__

=head1 NAME

Text::Tokenizer::Class::Constant



=head1 SYNOPSIS

    use Text::Tokenizer::Class::Constant;    # this will export some constants.
  



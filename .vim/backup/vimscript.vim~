" INCLUDE GUARD {{{1
if exists('g:loaded_<%filename_noext%>') && g:loaded_<%filename_noext%> != 0 | finish | endif
let g:loaded_<%filename_noext%> = 1
" }}}1
" SAVING CPO {{{1
let s:save_cpo = &cpo
set cpo&vim
" }}}1

" SCOPED VARIABLES {{{1
" }}}1
" GLOBAL VARIABLES {{{1
" }}}1

" FUNCTION DEFINITION {{{1

" s:warn(msg) {{{2
func! s:warn(msg)
    echohl WarningMsg
    echo a:msg
    echohl None
endfunc
" }}}2

" }}}1

" COMMAND {{{1
" }}}1

" MAPPING {{{1
" }}}1

" AUTOCOMMAND {{{1
" }}}1

" RESTORE CPO {{{1
let &cpo = s:save_cpo
" }}}1


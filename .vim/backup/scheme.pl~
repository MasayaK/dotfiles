#!/usr/bin/env perl
use strict;
use warnings;
use utf8;

use Pod::Usage;
use FileHandle;
use Getopt::Long qw(:config auto_help gnu_compat);


GetOptions(
    # switch here ...
) or pod2usage(-verbose => 2);


my $scmfile = shift;
my $FH = FileHandle->new($scmfile) or die $!;

while (defined(my $c = getc $FH)) {
    print $c;
}



__END__

=head1 NAME

    scheme.pl - scheme implementation in Perl

=head1 SYNOPSIS

    perl scheme.pl [switch] [script files] ...

=head1 OPTION

=over 1

=item -h, --help

show this help.


=back

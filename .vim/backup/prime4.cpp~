#include <iostream>
#include <cstdlib>
#include <utility>
#include <vector>


VecInt prime;    // キャッシュ


bool
isPrime(int n) {

    const int END_N = n / 2;


    if (! prime.empty()) {
        for (VecInt::iterator it = prime.begin(); it != prime.end(); ++it) {
            if (n % *it == 0) {
                return false;
            }
        }
        if (prime.back() > END_N) {
            prime.push_back(n);
            return true;
        }
    }
    for (int i = 2; i <= END_N; ++i) {
        if (n % i == 0) {
            return false;
        }
    }

    // 素数を見つけたらキャッシュしておく
    prime.push_back(n);
    return true;
}
